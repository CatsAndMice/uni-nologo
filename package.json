  "uni-app": {
    "scripts": {
      "dev:mp-dingtalk": {
        "title": "钉钉小程序-dev",
        "env": {
          "UNI_PLATFORM": "mp-alipay",
          "UNI_NODE_ENV": "dev"
        },
        "define": {
          "MP-DINGTALK": true
        }
      },
      "test:mp-dingtalk": {
        "title": "钉钉小程序-test",
        "env": {
          "UNI_PLATFORM": "mp-alipay",
          "UNI_NODE_ENV": "test"
        },
        "define": {
          "MP-DINGTALK": true
        }
      },
      "mp-dingtalk": {
        "title": "钉钉小程序",
        "env": {
          "UNI_PLATFORM": "mp-alipay",
          "UNI_NODE_ENV": "prod"
        },
        "define": {
          "MP-DINGTALK": true
        }
      }
    }
