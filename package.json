    "medash": "^3.3.0",
    "pinia": "^2.0.34",
    "uni-ajax": "^2.5.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.1.9"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-automator": "3.0.0-3070920230324001",
    "@dcloudio/uni-cli-shared": "3.0.0-3070920230324001",
    "@dcloudio/uni-stacktracey": "3.0.0-3070920230324001",
