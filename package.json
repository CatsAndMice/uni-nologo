{
  "name": "uni-jingdian",
  "version": "0.0.0",
  "scripts": {
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-dingtalk": "uni -p dev:mp-dingtalk",
    "test:mp-dingtalk": "uni -p test:mp-dingtalk",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-dingtalk": "uni build -p mp-dingtalk"
  },
  "dependencies": {
